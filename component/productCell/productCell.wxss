/* component/productCell.wxss */
.main {
  background-color: #fff;
  border-bottom: 20rpx solid #f1f1f1;
}
.icon {
  width:180rpx;
  height:180rpx;
  background-color:skyblue;
  flex-shrink: 0;
  border-radius: 3px;
}
.rightView {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 5rpx 26rpx;
}
.bottomView {
  padding: 0rpx 30rpx;
  display:flex;
  align-items: center;
  height: 80rpx;
  background-color: #fff;
  border-top: 1rpx solid #f1f1f1;
  justify-content: space-between;
  flex-direction: row;
}
.button {
  display:flex;
  align-self:center;
  text-align: center;
  height: 48rpx;
  width: 150rpx;
  font-size: 28rpx;
  justify-content: center;
  border-radius: 24rpx;
  align-items: center;
  color: #898b8f;
  border: 1rpx solid #ccc;
}
