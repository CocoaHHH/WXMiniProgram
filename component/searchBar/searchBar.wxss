/* component/searchBar/searchBar.wxss */
.searchView {
  background-color: #fff;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index:10;
  width: 100%;
  
}
.searchBG {
  width: 100%;
  border-radius: 30rpx;
  height: 60rpx;
  background-color: #f1f1f1;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin: 0rpx 20rpx;
}
.place {
  color: #999;
  font-size: 28rpx;
  margin-left: 5rpx;
  text-align: center;
}
.input {
  min-width:90%;
  color: #333;
  font-size: 28rpx;
  margin-left: 5rpx;
  margin-right: 10rpx;
  height: 60rpx;
  align-self:center;
}
.searchImage {
  width: 38rpx;
  height:38rpx;
}